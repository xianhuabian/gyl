		<!-- 
			无税金额
		 -->
		<property name="wsje"></property>
		<!-- 
			型号
		 -->
		<property name="xh"></property>
		<!-- 
			赠品
		 -->
		<property name="zp"></property>
		<many-to-one name="cgddzhub" column="cgddzhubid" class="com.sdibt.gyl.domain.business.cggl.Cgddzhub"></many-to-one>
	</class>
</hibernate-mapping>
